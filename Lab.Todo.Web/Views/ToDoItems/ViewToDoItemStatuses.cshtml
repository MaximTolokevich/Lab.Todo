@model IEnumerable<Lab.Todo.Web.Models.ToDoItemStatusModel>

@{
    ViewBag.Title = "Statuses";
    Layout = "_Layout";
}

<h2>List of Statuses</h2>

<table class="table">
    <tr>
        <th>@Html.DisplayNameFor(model => model.Name)</th>
        <th>Options</th>
    </tr>
    @foreach (var item in Model)
    {
<tr>
    <td>@Html.DisplayName(item.Name)</td>
</tr>
    }
</table>
<br/>